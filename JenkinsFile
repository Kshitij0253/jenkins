pipeline {
    agent any

    stages {
        stage('RUn A Command') {
            steps {
                echo 'Hello Run A Command'

            }
        }
         stage('Run on Build') {

            steps {
                echo 'RUn on BUild'
            }
        }
         stage('Run on Test') {
            steps {
                echo 'deploy on test'

            }
        }
     stage('Deploy on Prod') {
            steps {
                echo 'deploy on Prod'

            }
        }
}
}
